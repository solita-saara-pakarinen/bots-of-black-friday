    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^29.0.3",
    "prettier": "2.8.1",
    "ts-jest": "^29.0.1",
    "ts-node": "^10.9.1",
    "typescript": "4.9.4"
